{
  "name": "simovative/zeus",
  "description": "A Post-Redirect-Get specialised framework for PHP",
  "keywords": [
    "framework"
  ],
  "homepage": "https://github.com/Simovative-gmbh/zeus",
  "license": "MIT",
  "authors": [
    {
      "name": "Benedikt Schaller",
      "email": "benedikt.schaller@simovative.com"
    },
    {
      "name": "Markus NÃ¶renberg",
      "email": "markus.noerenberg@simovative.com"
    },
    {
      "name": "Sebastian Hartmann",
      "email": "sebastian.hartmann@simovative.com"
    }
  ],
  "type": "library",
  "autoload": {
    "classmap": [
      "src/",
      "skeleton/bundles"
    ]
  },
  "require": {
    "smarty/smarty": "v3.*",
    "php": ">=7.1.0",
    "ext-intl": "*",
    "symfony/debug": ">=v2.5.0",
    "symfony/console": ">=v2.5.0"
  },
  "require-dev": {
  },
  "suggest": {
    "ext-fileinfo": "Better recognition for file mime types in the file system classes."
  },
  "bin": [
    "bin/zeus"
  ],
  "archive": {
    "exclude": [
      "!vendor",
      "tests"
    ]
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    }
  }
}
